<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.minicapstone390">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MiniCapstone390">
        <activity
            android:name=".Views.SensorActivity"
            android:label="Sensor"
            android:exported="false" />
        <activity
            android:name=".Views.DeviceActivity"
            android:label="Device"
            android:exported="false" />
        <activity
            android:name=".Views.ProfileActivity"
            android:label="Profile"
            android:exported="false" />
        <activity
            android:name=".Views.MainActivity"
            android:label="Loading..."
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Views.LoginActivity"
            android:exported="true"
            android:label="Login" />
        <activity
            android:name=".Views.SignupActivity"
            android:exported="true"
            android:label="Signup" />
        <activity
            android:name=".Views.HomeActivity"
            android:exported="true"
            android:label="Home" />
    </application>

</manifest>